<Tab name="Ogolne" padding="4 4 4 4">
    <Box name="Autojoin and taunt" width="content" height="content" padding="12 6 6 6">
        <List width="200">
            <AutoVariable width="fill" target="autojoin.auto-queue" label="Autoqueue"/>
            <AutoVariable width="fill" target="autojoin.auto-requeue" label="Auto requeue"/>
            <AutoVariable width="fill" target="autoqueue.mode" label="Tryb autoqueue" min="0" max="12"/>
            <AutoVariable width="fill" target="hack.requeue-on-kick" label="Requeue gdy kicked"/>
            <AutoVariable width="fill" target="autotaunt.enable" label="Autotaunt wl"/>
            <LabeledObject width="fill" label="Autotauntuj z bronia">
                <Select target="autotaunt.auto-weapon">
                    <Option name="Zadna" value="0"/>
                    <Option name="Glowna" value="1"/>
                    <Option name="Poboczna" value="2"/>
                    <Option name="Melee" value="3"/>
                    <Option name="Zestawem do przebran" value="4"/>
                    <Option name="PDA" value="4"/>
                    <Option name="PDA zniszczenia" value="5"/>
                </Select>
            </LabeledObject>
            <AutoVariable width="fill" target="autotaunt.chance" label="Szansa na autotaunt" min="0" max="100.1" step="1"/>
            <AutoVariable width="fill" target="autotaunt.safety-distance" label="Dystans bezpiecznego autotaunta"/>
            <LabeledObject width="fill" label="Autojoin class" tooltip="Automatycznie wybierz klase.">
                <Select target="autojoin.class">
                    <Option name="Zadna" value="0"/>
                    <Option name="Skaut" value="1"/>
                    <Option name="Snajper" value="2"/>
                    <Option name="Zolnierz" value="3"/>
                    <Option name="Demoman" value="4"/>
                    <Option name="Medyk" value="5"/>
                    <Option name="Gruby" value="6"/>
                    <Option name="Pyro" value="7"/>
                    <Option name="Szpieg" value="8"/>
                    <Option name="Inzynier" value="9"/>
                </Select>
            </LabeledObject>
            <AutoVariable width="fill" target="autojoin.team" label="Druzynowy autojoin"
                          tooltip="Automatycznie dolacza do druzyny."/>
        </List>
    </Box>
    <Box padding="12 6 6 6" width="content" height="content" name="Inzynier-bot" y="165">
        <List width="200">
            <AutoVariable width="fill" target="autorepair.enabled" label="Wl auto naprawa"/>
            <AutoVariable width="fill" target="autorepair.silent" label="Silent"/>
            <AutoVariable width="fill" target="autorepair.sentry" label="Naprawiaj sentry"/>
            <AutoVariable width="fill" target="autorepair.dispenser" label="Naprawiaj dispenser"/>
            <AutoVariable width="fill" target="autorepair.teleport" label="Naprawiaj teleporter"/>
            <LabeledObject width="fill" label="Priorytet">
                <Select target="autorepair.priority">
                    <Option name="Zaden" value="0"/>
                    <Option name="Sentry" value="1"/>
                    <Option name="Dispenser" value="2"/>
                    <Option name="Teleport" value="3"/>
                </Select>
            </LabeledObject>
            <AutoVariable width="fill" target="autoupgrade.enabled" label="Wl auto ulepszanie"/>
            <AutoVariable width="fill" target="autoupgrade.sentry" label="Ulepszanie sentry"/>
            <AutoVariable width="fill" target="autoupgrade.sentry.level" label="Poziom ulepszenia sentry" min="1" max="3"/>
            <AutoVariable width="fill" target="autoupgrade.dispenser" label="Ulepszanie dispensera"/>
            <AutoVariable width="fill" target="autoupgrade.dispenser.level" label="Poziom ulepszenia dispensera" min="1"
                          max="3"/>
            <AutoVariable width="fill" target="autoupgrade.teleport" label="Ulepszanie teleporter"/>
            <AutoVariable width="fill" target="autoupgrade.teleport.level" label="Poziom ulepszenia teleportera" min="1"
                          max="3"/>
        </List>
    </Box>
    <Box name="Narzedzia" width="content" height="content" padding="12 6 6 6" x="215">
        <List width="200">
            <AutoVariable width="fill" target="cat-bot.enable" label="Wl narzedzia"/>
            <AutoVariable width="fill" target="player-tools.betrayal-limit" label="Limit zdrad"/>
            <AutoVariable width="fill" target="cat-bot.abandon-if.bots-gte" label="Porzuc jezeli boty Gte"/>
            <AutoVariable width="fill" target="cat-bot.abandon-if.ipc-bots-gte" label="Porzuc jezeli IPC bots Gte"/>
            <AutoVariable width="fill" target="cat-bot.abandon-if.humans-lte" label="Porzuc jezeli ludzie Lte"/>
            <AutoVariable width="fill" target="cat-bot.abandon-if.players-lte" label="Porzuc jezeli gracze Lte"/>
            <AutoVariable width="fill" target="cat-bot.auto-crouch" label="Autokucniecie"/>
            <AutoVariable width="fill" target="cat-bot.mark-human-after-kills" label="Zaznacz ludzi po killu"/>
            <AutoVariable width="fill" target="cat-bot.micspam.enable" label="Micspam"/>
            <AutoVariable width="fill" target="cat-bot.micspam.interval-off" label="Interwal micspamu wyl"/>
            <AutoVariable width="fill" target="cat-bot.micspam.interval-on" label="Interwal micspam wl"/>
            <AutoVariable width="fill" target="cat-bot.votekicks" label="Losowe votekicki"/>
            <AutoVariable width="fill" target="cat-bot.votekicks.rage-only" label="Rage only votekicks"/>
            <AutoVariable width="fill" target="cat-bot.always-crouch" label="Zawsze kucaj"/>
            <AutoVariable width="fill" target="cat-bot.autoreport" label="Auto reportuj"/>
            <AutoVariable width="fill" target="cat-bot.anti-autobalance" label="Anty auto balance"/>
            <AutoVariable width="fill" target="misc.autodisguise" label="Auto przebranie"/>
            <AutoVariable width="fill" target="cat-bot.anti-motd" label="Anty MOTD"/>
            <AutoVariable width="fill" target="cat-bot.autovote-map" label="Autovote na aktualna mape"/>
            <AutoVariable width="fill" target="misc.auto-noisemaker" label="Auto odblokuj i wyposaz noisemakery"/>
        </List>
    </Box>
</Tab>
